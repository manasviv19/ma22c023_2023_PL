      "source": [
        "def series_sum(i, N):\n",
        "    total_sum = 0\n",
        "    for n in range(1, N + 1):\n",
        "        total_sum += n * i\n",
        "    return total_sum\n",
        "\n",
        "i = int(input(\"Enter the value of i: \"))\n",
        "N = int(input(\"Enter the value of N: \"))\n",
        "result = series_sum(i, N)\n",
        "print(f\"The sum of the sequence is: {result}\")"
      ]
    }
  ]
